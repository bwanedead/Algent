{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:5173",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Algent",
    "version": "0.0.0"
  },
  "tauri": {
    "bundle": {
      "identifier": "com.algent.app"
    },
    "allowlist": {
      "http": {
        "scope": ["http://127.0.0.1:*"]
      },
      "shell": {
        "all": false
      }
    },
    "windows": [
      {
        "title": "Algent Lab",
        "width": 1100,
        "height": 800,
        "resizable": true
      }
    ]
  }
}
